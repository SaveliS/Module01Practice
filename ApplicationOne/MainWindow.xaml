<Window x:Class="ApplicationOne.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ApplicationOne"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="30"/> <!--WindowName-->
            <RowDefinition Height="200"/> <!--Logo-->
            <RowDefinition/> <!--Content-->
        </Grid.RowDefinitions>

        <!--#region Window name -->
        <Grid Grid.Row="0">
            <TextBlock Text="Login" FontSize="15" FontWeight="SemiBold"/>
        </Grid>
        <!--#endregion-->
    </Grid>
</Window>
